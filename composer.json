{
    "name": "nova-kit/nova-field-mixins",
    "description": "Laravel Nova Field Mixins",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],

    "autoload": {
        "psr-4": {
            "NovaKit\\Fields\\Mixins\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaKit\\Fields\\Mixins\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "illuminate/support": "^8.67 || ^9.0"
    },
    "require-dev": {
        "laravel/nova": "^4.0",
        "nunomaduro/larastan": "^1.0.1 || ^2.0.1",
        "orchestra/canvas": "^6.2 || ^7.0",
        "orchestra/testbench": "^6.25.1 || ^7.10.1"
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
          "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "NovaKit\\Fields\\Mixins\\MixinServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
